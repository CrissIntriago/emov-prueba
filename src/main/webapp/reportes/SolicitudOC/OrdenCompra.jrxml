	<queryString>
		<![CDATA[select sol.id as id_orden,ad.id AS id_adqui,sol.codigo_orden ,
sol.elaboracion_doc, sol.detalle_entrega,
sol.detalle_garantia,
u.nombre, ci.texto as tipoAdqui,sol.objeto_contratacion,sol.fecha_elaboracion,
ad.garantia,cl.apellido as apellidoAut,cl.nombre as nombreAut,cl.identificacion as icAut,
cl.ruc as rucAut,cli.direccion as direccionProveedor,
cli.apellido as apellidoProveedor,cli.nombre as nombreProveedor,cli.identificacion as icProveedor,u.nombre as unidad,
cli.ruc as rucProveedor,sol.detalle_consideracion,sol.detalle_valor_adjudicado,
sol.nuevo_concepto,sol.detalle_concepto,sol.detalle_certificacion,sol.detalle_presupuesto,
sol.plazo_entrerga,sol.condicion_plazo,ce.texto as tipo_plazo, sol.dia_plazo,sol.detalle_garantia,sol.detalle_obligacion,
sol.multa,sol.obligaciones,sol.detalle_vigencia,sol.detalle_controversia,
sol.detalle_terminacion_orden,sol.detalle_notificacion,
ad.garantia as garantia,
sol.obligaciones, sol.num_cotizacion, cl.email as email_prov,
LPAD(sc.secuencial::text,5,'0')||'-'||sc.periodo as cert_presupuestaria
from administracion.solicitud_orden_compra sol
inner join talento_humano.servidor s ON s.id = sol.autorizador
inner join cliente cl ON cl.id = s.persona
inner join activos.adquisiciones ad ON ad.id = sol.adquisicion
inner join catalogo_item ci ON ci.id = ad.tipo_adquisicion
inner join unidad_administrativa u ON sol.unidad = u.id
inner join proveedor pro on pro.id = ad.proveedor
inner join cliente cli ON cli.id = pro.cliente
inner join certificacion_presupuestaria_anual.solicitud_reserva_compromiso sc on sc.id = sol.reserva_compromiso
inner join catalogo_item ce ON ce.id = sol.tipo_plazo
where sol.id = $P{id_orden}]]>
	</queryString>
